    "reflect-metadata": "^0.1.10",
    "sqlite": "^2.8.0"
  },
  "devDependencies": {
    "@types/form-data": "0.0.33",
    "@types/koa": "^2.0.39",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.2",
    "@types/shelljs": "^0.7.2",
    "form-data": "^2.2.0",
