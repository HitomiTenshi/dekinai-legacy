    "url": "https://www.dekinai.moe"
  },
  "license": "MIT",
  "dependencies": {
    "http-shutdown": "^1.2.0",
    "inversify": "^4.1.1",
    "koa": "^2.2.0",
    "koa-busboy": "^1.1.0",
    "reflect-metadata": "^0.1.10",
    "sqlite": "^2.8.0"
  },
  "devDependencies": {
    "@types/koa": "^2.0.39",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.29",
    "@types/shelljs": "^0.7.2",
    "mocha": "^3.4.2",
    "shelljs": "^0.7.8",
    "shx": "^0.2.2",
    "typescript": "^2.3.4"
  }
