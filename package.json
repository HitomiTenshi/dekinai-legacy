{
  "name": "dekinai",
  "version": "2.0.0",
  "description": "A highly configurable self-hosted file storage server",
  "main": "main.js",
  "scripts": {
    "postinstall": "shx cp -n template/config.json . && shx cp template/config.json tests/ && shx mkdir -p uploads dekinai tests/uploads tests/dekinai",
    "start": "tsc && node main.js",
    "test": "tsc && cd tests && mocha --exit .",
    "codacy": "nyc --reporter=text-lcov npm run test | codacy-coverage --language typescript",
    "nyc": "nyc --reporter=html npm run test",
    "rollup": "tsc -p bundle/tsconfig-rollup.json && rollup -c bundle/rollup.js",
    "minify": "uglifyjs --compress properties,dead_code,drop_debugger,unsafe,unsafe_comps,unsafe_Func,unsafe_math,unsafe_proto,unsafe_regexp,conditionals,comparisons,evaluate,booleans,loops,unused,toplevel,hoist_funs,if_return,join_vars,cascade,collapse_vars,reduce_vars,negate_iife,pure_getters,passes=2 --mangle toplevel,eval --toplevel --source-map content=dekinai.js.map --output dekinai.min.js dekinai.js",
    "release": "npm run rollup && npm run minify"
  },
  "keywords": [
    "dekinai",
    "dekinai.moe"
  ],
  "author": {
    "name": "Johann Rekowski",
    "email": "johann.rekowski@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HitomiTenshi/Dekinai.git"
  },
  "license": "MIT",
  "dependencies": {
    "inversify": "^4.3.0",
    "koa": "^2.3.0",
    "koa-busboy": "^1.1.1",
    "reflect-metadata": "^0.1.10",
    "sqlite": "^2.8.0"
  },
  "devDependencies": {
    "@types/form-data": "^2.2.0",
    "@types/koa": "^2.0.39",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.34",
    "@types/shelljs": "^0.7.4",
    "codacy-coverage": "^2.0.3",
    "form-data": "^2.3.1",
    "got": "^7.1.0",
    "mocha": "^4.0.1",
    "nyc": "^11.2.1",
    "rollup": "^0.50.0",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript2": "0.7.0",
    "shelljs": "^0.7.8",
    "shx": "^0.2.2",
    "typescript": "^2.5.3",
    "uglify-es": "^3.1.3"
  }
}
