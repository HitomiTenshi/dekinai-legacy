    "inversify": "^4.1.1",
    "koa": "^2.2.0",
    "koa-busboy": "^1.0.1",
    "mz": "^2.6.0",
    "reflect-metadata": "^0.1.10",
    "sqlite3": "^3.1.8"
  },
  "devDependencies": {
    "@types/koa": "^2.0.39",
    "@types/mocha": "^2.2.41",
    "@types/mz": "0.0.31",
