{
  "name": "dekinai.moe",
  "version": "1.0.0",
  "description": "A highly configurable self hosted file storage server",
  "main": "index.js",
  "scripts": {
    "postinstall": "shx cp -n template/config.json . && shx cp template/config.json test/ && shx mkdir -p uploads temp test/uploads test/temp",
    "start": "tsc && node index.js",
    "test": "tsc && cd test && mocha ."
